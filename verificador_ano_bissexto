def verificador_ano_bissexto():
    ano = int(input())

# TODO: Verifique se o ano é bissexto utilizando uma estrutura de controle condicional, como if/else:

    if (ano % 4 == 0 and ano % 100 != 0) or ano % 400 == 0:
        return "SIM"
  
    else:
        return "NÃO"
  
print(verificador_ano_bissexto())
